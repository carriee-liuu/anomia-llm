# Frontend Environment Variables
# Copy this file to .env.local for development

# Backend API URL (Production - Railway)
REACT_APP_BACKEND_URL=https://your-railway-app.railway.app
REACT_APP_WS_URL=wss://your-railway-app.railway.app

# Development URLs (uncomment for local development)
# REACT_APP_BACKEND_URL=http://localhost:3001
# REACT_APP_WS_URL=ws://localhost:3001

# Game Configuration
REACT_APP_MAX_PLAYERS=8
REACT_APP_DEFAULT_ROOM_EXPIRY=24

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG=false

# Build Configuration
REACT_APP_VERSION=1.0.0
REACT_APP_BUILD_ENV=production

